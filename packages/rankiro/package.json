{
    "name": "@frommiddle/rankiro",
    "version": "1.0.0",
    "description": "Core business logic package for Rankiro video ranking platform",
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js"
        },
        "./ranking": {
            "types": "./dist/ranking/index.d.ts",
            "import": "./dist/ranking/index.js"
        },
        "./analytics": {
            "types": "./dist/analytics/index.d.ts",
            "import": "./dist/analytics/index.js"
        },
        "./data": {
            "types": "./dist/data/index.d.ts",
            "import": "./dist/data/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc --watch",
        "dev": "tsc --watch",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src --ext .ts,.tsx",
        "lint:fix": "eslint src --ext .ts,.tsx --fix",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "keywords": [
        "rankiro",
        "video",
        "ranking",
        "analytics",
        "typescript"
    ],
    "author": "Rankiro Team",
    "license": "MIT",
    "dependencies": {},
    "devDependencies": {
        "@frommiddle/tsconfig": "workspace:*",
        "@frommiddle/eslint-config": "workspace:*",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.10.2",
        "eslint": "^9.35.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.9.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}